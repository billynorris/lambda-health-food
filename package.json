{
  "name": "lambda-mfp-food",
  "version": "1.0.2",
  "description": "A lambda to scrape food data from MyFitnessPal",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "files": [
    "bin/**/*"
  ],
  "scripts": {
    "build": "node node_modules/typescript/lib/tsc.js",
    "prepare": "node node_modules/typescript/lib/tsc.js",
    "test-only": "mocha --require ts-node/register tests/**/**/*.ts",
    "test": "nyc npm run test-only -- --reporter mocha-junit-reporter"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:billynorris/lambda-mfp-food.git"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cheerio": "^0.22.22",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/sinon": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "aws-sdk": "^2.751.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.1.3",
    "mocha-junit-reporter": "^2.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "sinon": "^9.1.0",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@dazn/lambda-powertools-logger": "^1.21.0",
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.0",
    "cheerio": "^1.0.0-rc.3",
    "http-errors": "^1.8.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "airbnb-typescript/base"
    ],
    "parserOptions": {
      "project": "./tsconfig-linting.json"
    },
    "rules": {
      "implicit-arrow-linebreak": "off",
      "no-console": "error"
    },
    "ignorePatterns": [
      "node_modules",
      "bin",
      "coverage"
    ]
  }
}
